#!/data/data/com.termux/files/usr/bin/bash
trap '' SIGINT
WIKI="https://wiki.termux.com/wiki/Hacking"
LIB="$PREFIX/lib/libmetasploit.so"
spinstep(){ sleep 1 && echo -ne "\e[1D"; }
echo -n "[*] Starting Metasploit Framework Console..... "
for i in {1..5}
do
	echo -n "|"
	spinstep
	echo -n "/"
	spinstep
	echo -n "-"
	spinstep
	echo -n "\\"
	spinstep
done
printf "\n"
play-audio $LIB
echo "[*] You have been rickrolled!"
echo "[*] Remember that Termux isn't for hacking, please read $WIKI"

